    }],
    "parameters": [{
        "name":"templateType",
        "required":"true"
    }, {
        "name":"fileName",
        "required":"true"
    }, {
        "name":"projectName",
        "required":"true"
    }, {
        "name":"packageName",
        "required":"true"
    }, {  
        "name":"tableName",
        "required":"true"
    }, {  
        "name":"columns",
        "required":"true"
    }]
